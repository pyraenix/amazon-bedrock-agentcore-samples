# Strands with AgentCore Browser Tool - Environment Configuration Template
# Copy this file to .env and fill in your actual values

# =============================================================================
# AWS Configuration
# =============================================================================

# AWS Credentials (use AWS CLI profile or IAM roles in production)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_SESSION_TOKEN=your_session_token_if_using_temporary_credentials

# AWS Region for AgentCore Browser Tool
AWS_DEFAULT_REGION=us-east-1
AWS_REGION=us-east-1

# =============================================================================
# Amazon Bedrock Configuration
# =============================================================================

# Primary Bedrock Model Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_EMBEDDING_MODEL_ID=amazon.titan-embed-text-v1

# Secondary Bedrock Models for Multi-Model Routing
BEDROCK_FALLBACK_MODEL_ID=anthropic.claude-3-haiku-20240307-v1:0
BEDROCK_HIGH_SECURITY_MODEL_ID=anthropic.claude-3-opus-20240229-v1:0

# Bedrock Runtime Endpoint (optional, uses default if not specified)
BEDROCK_RUNTIME_ENDPOINT=https://bedrock-runtime.us-east-1.amazonaws.com

# =============================================================================
# AgentCore Browser Tool Configuration
# =============================================================================

# AgentCore Browser Tool Region
AGENTCORE_REGION=us-east-1

# Browser Session Configuration
BROWSER_SESSION_TIMEOUT=300
BROWSER_MAX_CONCURRENT_SESSIONS=10
BROWSER_ENABLE_OBSERVABILITY=true

# Security Configuration
BROWSER_SECURITY_LEVEL=high
BROWSER_NETWORK_ISOLATION=true
BROWSER_SCREENSHOT_REDACTION=true
BROWSER_AUDIT_LOGGING=true

# =============================================================================
# Strands Framework Configuration
# =============================================================================

# Strands Agent Configuration
STRANDS_AGENT_NAME=strands-agentcore-tutorial
STRANDS_AGENT_VERSION=1.0.0
STRANDS_LOG_LEVEL=INFO

# Multi-LLM Provider Configuration
STRANDS_PRIMARY_LLM_PROVIDER=bedrock
STRANDS_FALLBACK_LLM_PROVIDER=openai
STRANDS_ENABLE_LLM_FALLBACK=true

# OpenAI Configuration (for multi-LLM support)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_ORGANIZATION=your_openai_org_id

# Anthropic Configuration (direct API, optional)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Ollama Configuration (for local models)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2:7b

# =============================================================================
# Strands Tool Configuration
# =============================================================================

# Custom Tool Registry
STRANDS_TOOL_REGISTRY_PATH=./tools
STRANDS_ENABLE_CUSTOM_TOOLS=true

# Browser Tool Configuration
STRANDS_BROWSER_TOOL_ENABLED=true
STRANDS_BROWSER_TOOL_TIMEOUT=60
STRANDS_BROWSER_TOOL_MAX_RETRIES=3

# Security Tool Configuration
STRANDS_PII_DETECTION_ENABLED=true
STRANDS_CREDENTIAL_INJECTION_ENABLED=true
STRANDS_AUDIT_TRAIL_ENABLED=true

# =============================================================================
# Security and Encryption Configuration
# =============================================================================

# Encryption Key for Sensitive Data (generate a secure key)
ENCRYPTION_KEY=your_32_character_encryption_key_here

# PII Detection Configuration
PII_DETECTION_ENABLED=true
PII_MASKING_ENABLED=true
PII_CONFIDENCE_THRESHOLD=0.8
PII_DETECTION_MODELS=en_core_web_sm

# Credential Management
CREDENTIAL_ENCRYPTION_ENABLED=true
CREDENTIAL_ROTATION_ENABLED=false
CREDENTIAL_STORAGE_TYPE=aws_secrets_manager

# AWS Secrets Manager Configuration
AWS_SECRETS_MANAGER_REGION=us-east-1
AWS_SECRETS_MANAGER_PREFIX=strands-tutorial/

# =============================================================================
# Workflow and Orchestration Configuration
# =============================================================================

# Workflow Engine Configuration
STRANDS_WORKFLOW_ENGINE_ENABLED=true
STRANDS_WORKFLOW_STATE_ENCRYPTION=true
STRANDS_WORKFLOW_CHECKPOINT_ENABLED=true

# Multi-Agent Coordination
STRANDS_MULTI_AGENT_ENABLED=true
STRANDS_AGENT_ISOLATION_ENABLED=true
STRANDS_SHARED_SESSION_POOL=true

# Session Pool Management
SESSION_POOL_SIZE=10
SESSION_POOL_MAX_IDLE_TIME=300
SESSION_POOL_CLEANUP_INTERVAL=60

# =============================================================================
# Monitoring and Observability Configuration
# =============================================================================

# Logging Configuration
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_FILE_PATH=./logs/strands_agentcore.log

# Metrics and Monitoring
ENABLE_METRICS=true
METRICS_PORT=8080
PROMETHEUS_ENABLED=false

# Audit Logging
AUDIT_LOG_ENABLED=true
AUDIT_LOG_PATH=./logs/audit.log
AUDIT_LOG_RETENTION_DAYS=90

# Multi-LLM Monitoring
LLM_PROVIDER_METRICS_ENABLED=true
LLM_COST_TRACKING_ENABLED=true
LLM_PERFORMANCE_MONITORING=true

# =============================================================================
# Development and Testing Configuration
# =============================================================================

# Development Mode (enables additional logging and debugging)
DEVELOPMENT_MODE=true
DEBUG_BROWSER_SESSIONS=false
DEBUG_LLM_REQUESTS=false

# Testing Configuration
TEST_MODE=false
MOCK_BROWSER_SESSIONS=false
MOCK_LLM_PROVIDERS=false
TEST_DATA_PATH=./test_data

# Jupyter Notebook Configuration
JUPYTER_ENABLE_EXTENSIONS=true
JUPYTER_LOG_LEVEL=WARNING

# =============================================================================
# Application-Specific Configuration
# =============================================================================

# Tutorial Configuration
TUTORIAL_DATA_PATH=./tutorial_data
TUTORIAL_EXAMPLES_PATH=./examples
TUTORIAL_ASSETS_PATH=./assets

# Data Processing
MAX_DOCUMENT_SIZE=10485760  # 10MB
MAX_DOCUMENTS_PER_BATCH=100
DOCUMENT_PROCESSING_TIMEOUT=120

# =============================================================================
# Multi-LLM Security Configuration
# =============================================================================

# Provider-Specific Security Policies
BEDROCK_SECURITY_LEVEL=high
OPENAI_SECURITY_LEVEL=medium
ANTHROPIC_SECURITY_LEVEL=high
OLLAMA_SECURITY_LEVEL=low

# Data Residency Configuration
BEDROCK_DATA_RESIDENCY=us-east-1
OPENAI_DATA_RESIDENCY=global
ANTHROPIC_DATA_RESIDENCY=us

# Cross-Provider Audit Configuration
CROSS_PROVIDER_AUDIT_ENABLED=true
PROVIDER_FAILOVER_AUDIT_ENABLED=true

# =============================================================================
# Compliance and Data Governance
# =============================================================================

# Data Retention Configuration
DATA_RETENTION_DAYS=30
SENSITIVE_DATA_RETENTION_DAYS=7
LOG_RETENTION_DAYS=90

# Compliance Configuration
GDPR_COMPLIANCE_ENABLED=true
HIPAA_COMPLIANCE_ENABLED=false
PCI_COMPLIANCE_ENABLED=false

# Industry-Specific Compliance
HEALTHCARE_COMPLIANCE_MODE=false
FINANCIAL_COMPLIANCE_MODE=false
LEGAL_COMPLIANCE_MODE=false

# Data Classification
DEFAULT_DATA_CLASSIFICATION=internal
SENSITIVE_DATA_CLASSIFICATION=confidential
PII_DATA_CLASSIFICATION=restricted

# =============================================================================
# Performance and Resource Configuration
# =============================================================================

# Memory and CPU Limits
MAX_MEMORY_USAGE=4096  # MB
MAX_CPU_USAGE=80       # Percentage

# Concurrency Configuration
MAX_CONCURRENT_OPERATIONS=20
MAX_CONCURRENT_AGENTS=5
ASYNC_TIMEOUT=30

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL=3600  # seconds
CACHE_MAX_SIZE=1000  # entries

# LLM Provider Performance Configuration
LLM_REQUEST_TIMEOUT=30
LLM_MAX_RETRIES=3
LLM_RETRY_DELAY=1

# =============================================================================
# Custom Tool Development Configuration
# =============================================================================

# Tool Development Environment
CUSTOM_TOOL_DEVELOPMENT_MODE=true
TOOL_VALIDATION_ENABLED=true
TOOL_SANDBOXING_ENABLED=true

# Tool Registry Configuration
TOOL_REGISTRY_AUTO_DISCOVERY=true
TOOL_REGISTRY_VALIDATION_STRICT=true

# Security Tool Configuration
SECURITY_TOOL_VALIDATION_ENABLED=true
SECURITY_POLICY_ENFORCEMENT=strict