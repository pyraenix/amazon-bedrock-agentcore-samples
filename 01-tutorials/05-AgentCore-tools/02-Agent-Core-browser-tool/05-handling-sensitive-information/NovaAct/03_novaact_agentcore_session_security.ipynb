    "    \n",
    "    Yields:\n",
    "        Tuple of (session_manager, observability) for secure operations\n",
    "    \"\"\"\n",
    "    session_manager = None\n",
    "    observability = None\n",
    "    cleanup_start_time = None\n",
    "    \n",
    "    try:\n",
    "        logger.info(\"üöÄ Initializing secure NovaAct-AgentCore session with resource management\")\n",
    "        \n",
    "        # Create session manager with monitoring\n",
    "        session_manager = NovaActAgentCoreSessionManager(\n",
    "            region=region,\n",
    "            enable_monitoring=enable_monitoring\n",
    "        )\n",
    "        \n",
    "        # Enter session context\n",
    "        session_manager.__enter__()\n",
    "        logger.info(\"‚úÖ AgentCore session created with automatic resource management\")\n",
    "        \n",
    "        # Initialize observability if enabled\n",
    "        if enable_observability:\n",
    "            observability = NovaActAgentCoreObservability(session_manager)\n",
    "            logger.info(\"üìä Observability monitoring enabled\")\n",
    "        \n",
    "        # Record session initialization\n",
    "        if observability:\n",
    "            observability._record_session_event('session_initialized', {\n",
    "                'region': region,\n",
    "                'monitoring_enabled': enable_monitoring,\n",
    "                'observability_enabled': enable_observability,\n",
    "                'automatic_cleanup_enabled': True,\n",
