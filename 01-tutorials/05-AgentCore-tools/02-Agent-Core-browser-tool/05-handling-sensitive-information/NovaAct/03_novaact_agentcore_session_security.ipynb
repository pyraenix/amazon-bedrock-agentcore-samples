    "        self.metrics['performance_metrics'].append(metric_data)\n",
    "    \n",
    "    def _calculate_success_rate(self) -> float:\n",
    "        \"\"\"Calculate operation success rate.\"\"\"\n",
    "        if self.metrics['operations_count'] == 0:\n",
    "            return 0.0\n",
    "        return (self.metrics['successful_operations'] / self.metrics['operations_count']) * 100\n",
    "    \n",
    "    def _calculate_average_operation_time(self) -> float:\n",
    "        \"\"\"Calculate average operation time.\"\"\"\n",
    "        if not self.metrics['performance_metrics']:\n",
    "            return 0.0\n",
