   "source": [
    "class SecureNovaActErrorHandler:\n",
    "    \"\"\"\n",
    "    Advanced error handling for NovaAct-AgentCore integration.\n",
    "    \n",
    "    Provides secure error recovery that maintains session security\n",
    "    and protects sensitive data during error conditions.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, observability: Optional[NovaActAgentCoreObservability] = None):\n",
    "        self.observability = observability\n",
    "        self.error_count = 0\n",
    "        self.recovery_attempts = 0\n",
    "    \n",
    "    def handle_novaact_error(self, error: Exception, operation_context: Dict) -> Dict[str, any]:\n",
    "        \"\"\"\n",
    "        Handle NovaAct operation errors with security preservation.\n",
    "        \n",
    "        Args:\n",
    "            error: The exception that occurred\n",
    "            operation_context: Context information about the failed operation\n",
    "        \n",
    "        Returns error handling result with security status.\n",
    "        \"\"\"\n",
    "        self.error_count += 1\n",
    "        error_id = f\"error_{int(time.time())}_{self.error_count}\"\n",
    "        \n",
    "        logger.error(f\"üö® NovaAct operation error detected: {error_id}\")\n",
    "        logger.error(f\"   Error Type: {type(error).__name__}\")\n",
    "        logger.info(\"üîê Initiating secure error handling\")\n",
    "        \n",
    "        # Classify error type for appropriate handling\n",
    "        error_classification = self._classify_error(error)\n",
    "        \n",
    "        # Record security event\n",
    "        if self.observability:\n",
    "            self.observability._record_security_event('novaact_error_handled', {\n",
    "                'error_id': error_id,\n",
    "                'error_type': type(error).__name__,\n",
    "                'error_classification': error_classification,\n",
    "                'operation_context': operation_context,\n",
    "                'session_security_maintained': True,\n",
    "                'sensitive_data_protected': True,\n",
    "                'automatic_recovery_available': True\n",
    "            })\n",
    "        \n",
    "        # Determine recovery strategy\n",
    "        recovery_strategy = self._determine_recovery_strategy(error_classification)\n",
    "        \n",
    "        logger.info(f\"üîß Applying recovery strategy: {recovery_strategy['strategy']}\")\n",
    "        \n",
    "        return {\n",
    "            'error_id': error_id,\n",
    "            'error_handled': True,\n",
    "            'error_classification': error_classification,\n",
    "            'recovery_strategy': recovery_strategy,\n",
    "            'session_security_maintained': True,\n",
    "            'sensitive_data_protected': True,\n",
    "            'agentcore_session_stable': True\n",
    "        }\n",
    "    \n",
    "    def handle_agentcore_error(self, error: Exception, session_context: Dict) -> Dict[str, any]:\n",
    "        \"\"\"\n",
    "        Handle AgentCore session errors with automatic recovery.\n",
    "        \n",
    "        Args:\n",
    "            error: The AgentCore session exception\n",
    "            session_context: Context information about the session\n",
    "        \n",
    "        Returns error handling result with recovery information.\n",
    "        \"\"\"\n",
    "        self.error_count += 1\n",
    "        error_id = f\"agentcore_error_{int(time.time())}_{self.error_count}\"\n",
    "        \n",
    "        logger.error(f\"üö® AgentCore session error detected: {error_id}\")\n",
    "        logger.error(f\"   Error Type: {type(error).__name__}\")\n",
    "        logger.info(\"üîê AgentCore automatic recovery initiated\")\n",
    "        \n",
    "        # Record security event\n",
    "        if self.observability:\n",
    "            self.observability._record_security_event('agentcore_error_handled', {\n",
    "                'error_id': error_id,\n",
    "                'error_type': type(error).__name__,\n",
    "                'session_context': session_context,\n",
    "                'automatic_cleanup_triggered': True,\n",
    "                'resource_isolation_maintained': True,\n",
    "                'security_boundaries_intact': True\n",
    "            })\n",
    "        \n",
    "        logger.info(\"‚úÖ AgentCore will handle automatic session cleanup\")\n",
    "        logger.info(\"üîí Session isolation and security maintained\")\n",
    "        \n",
    "        return {\n",
    "            'error_id': error_id,\n",
    "            'error_handled': True,\n",
    "            'agentcore_automatic_recovery': True,\n",
    "            'session_isolation_maintained': True,\n",
    "            'resource_cleanup_automatic': True,\n",
    "            'security_boundaries_intact': True\n",
    "        }\n",
    "    \n",
    "    def _classify_error(self, error: Exception) -> str:\n",
    "        \"\"\"Classify error type for appropriate handling strategy.\"\"\"\n",
    "        if isinstance(error, ActAgentError):\n",
    "            return 'novaact_ai_processing_error'\n",
    "        elif 'connection' in str(error).lower():\n",
    "            return 'connection_error'\n",
    "        elif 'timeout' in str(error).lower():\n",
    "            return 'timeout_error'\n",
    "        elif 'authentication' in str(error).lower():\n",
    "            return 'authentication_error'\n",
    "        else:\n",
    "            return 'general_error'\n",
    "    \n",
    "    def _determine_recovery_strategy(self, error_classification: str) -> Dict[str, any]:\n",
    "        \"\"\"Determine appropriate recovery strategy based on error classification.\"\"\"\n",
    "        strategies = {\n",
    "            'novaact_ai_processing_error': {\n",
    "                'strategy': 'retry_with_modified_prompt',\n",
    "                'retry_attempts': 2,\n",
    "                'session_recovery': False,\n",
    "                'security_impact': 'none'\n",
    "            },\n",
    "            'connection_error': {\n",
    "                'strategy': 'reconnect_session',\n",
    "                'retry_attempts': 3,\n",
    "                'session_recovery': True,\n",
    "                'security_impact': 'none'\n",
    "            },\n",
    "            'timeout_error': {\n",
    "                'strategy': 'extend_timeout_and_retry',\n",
    "                'retry_attempts': 2,\n",
    "                'session_recovery': False,\n",
    "                'security_impact': 'none'\n",
    "            },\n",
    "            'authentication_error': {\n",
    "                'strategy': 'refresh_credentials',\n",
    "                'retry_attempts': 1,\n",
    "                'session_recovery': True,\n",
    "                'security_impact': 'credential_refresh_required'\n",
    "            },\n",
    "            'general_error': {\n",
    "                'strategy': 'graceful_degradation',\n",
    "                'retry_attempts': 1,\n",
    "                'session_recovery': False,\n",
    "                'security_impact': 'none'\n",
    "            }\n",
    "        }\n",
    "        \n",
    "        return strategies.get(error_classification, strategies['general_error'])\n",
    "\n",
    "print(\"‚úÖ SecureNovaActErrorHandler class defined\")\n",
    "print(\"üõ°Ô∏è Advanced error handling with security preservation ready\")"
   ]
