    "def secure_login_with_novaact_agentcore(\n",
    "    login_url: str,\n",
    "    username: str,\n",
    "    password: str,\n",
    "    region: str = 'us-east-1'\n",
    ") -> Dict:\n",
    "    \"\"\"\n",
    "    Demonstrate secure login automation using NovaAct within AgentCore's managed browser.\n",
    "    \n",
    "    This function shows how NovaAct's AI model processes natural language login\n",
    "    instructions within AgentCore's containerized, secure browser environment.\n",
    "    \n",
    "    Key Security Features:\n",
    "    - Credentials never leave AgentCore's isolated browser session\n",
    "    - NovaAct AI processing happens within the managed environment\n",
    "    - Automatic session cleanup protects against data leakage\n",
    "    - All browser traffic is contained within AgentCore's infrastructure\n",
    "    \n",
    "    Args:\n",
    "        login_url: URL of the login page\n",
    "        username: Login username (handled securely)\n",
    "        password: Login password (handled securely)\n",
    "        region: AWS region for AgentCore\n",
    "        \n",
    "    Returns:\n",
    "        Dict containing login results and security status\n",
    "    \"\"\"\n",
    "    login_result = {\n",
    "        'navigation_successful': False,\n",
    "        'login_attempted': False,\n",
    "        'login_successful': False,\n",
    "        'session_isolated': True,  # Always true with AgentCore\n",
    "        'credentials_protected': True,  # Always true with AgentCore\n",
    "        'agentcore_managed': True,  # Always true with AgentCore\n",
    "        'error': None,\n",
    "        'security_notes': []\n",
    "    }\n",
    "    \n",
    "    try:\n",
    "        logger.info(f\"üîê Starting secure login automation for: {login_url}\")\n",
    "        logger.info(\"üõ°Ô∏è  All operations will be contained within AgentCore's isolated environment\")\n",
    "        \n",
    "        # Create AgentCore browser session with full isolation\n",
    "        with browser_session(region=region) as agentcore_client:\n",
    "            logger.info(\"‚úÖ AgentCore browser session created with containerized isolation\")\n",
    "            login_result['security_notes'].append(\"Browser runs in isolated container\")\n",
    "            \n",
    "            # Get secure CDP connection\n",
    "            ws_url, headers = agentcore_client.generate_ws_headers()\n",
    "            logger.info(\"üîó Secure CDP connection established to AgentCore browser\")\n",
    "            \n",
    "            # Connect NovaAct to AgentCore's secure browser infrastructure\n",
    "            with NovaAct(\n",
    "                cdp_endpoint_url=ws_url,\n",
    "                cdp_headers=headers,\n",
    "                nova_act_api_key=os.environ['NOVA_ACT_API_KEY']\n",
    "            ) as nova_act:\n",
    "                logger.info(\"ü§ñ NovaAct AI connected to AgentCore's secure browser session\")\n",
    "                login_result['security_notes'].append(\"NovaAct AI processing within managed environment\")\n",
    "                \n",
    "                # Step 1: Navigate to login page using natural language\n",
    "                logger.info(f\"üåê Navigating to login page: {login_url}\")\n",
    "                navigation_result = nova_act.act(\n",
    "                    f\"Navigate to the login page at {login_url} and wait for it to fully load\"\n",
    "                )\n",
    "                \n",
    "                if navigation_result.success:\n",
    "                    logger.info(\"‚úÖ Successfully navigated to login page\")\n",
    "                    login_result['navigation_successful'] = True\n",
    "                    login_result['security_notes'].append(\"Navigation contained within AgentCore session\")\n",
    "                    \n",
    "                    # Step 2: Perform login using natural language instructions\n",
    "                    logger.info(\"üîë Attempting secure login with NovaAct AI...\")\n",
    "                    logger.info(\"üõ°Ô∏è  Credentials will be processed securely within the isolated browser\")\n",
    "                    \n",
    "                    # NovaAct processes the login instruction with AI\n",
    "                    login_instruction = (\n",
    "                        f\"Please log in to this website using the following credentials: \"\n",
    "                        f\"Username: '{username}' and Password: '{password}'. \"\n",
    "                        f\"Find the username and password fields, fill them in, and submit the login form.\"\n",
    "                    )\n",
    "                    \n",
    "                    login_attempt_result = nova_act.act(login_instruction)\n",
    "                    login_result['login_attempted'] = True\n",
    "                    login_result['security_notes'].append(\"Credentials processed by AI within isolated session\")\n",
    "                    \n",
    "                    if login_attempt_result.success:\n",
    "                        logger.info(\"‚úÖ Login automation completed successfully!\")\n",
    "                        login_result['login_successful'] = True\n",
    "                        login_result['security_notes'].append(\"Login completed within secure environment\")\n",
    "                        \n",
    "                        # Step 3: Verify login success (optional)\n",
    "                        logger.info(\"üîç Verifying login success...\")\n",
    "                        verification_result = nova_act.act(\n",
    "                            \"Check if the login was successful by looking for signs that we are now logged in, \"\n",
    "                            \"such as a dashboard, welcome message, or logout button\"\n",
    "                        )\n",
    "                        \n",
    "                        if verification_result.success:\n",
    "                            logger.info(\"‚úÖ Login verification successful - user is logged in\")\n",
    "                            login_result['security_notes'].append(\"Login verification completed securely\")\n",
    "                        else:\n",
    "                            logger.warning(\"‚ö†Ô∏è  Could not verify login success, but login attempt completed\")\n",
    "                    \n",
    "                    else:\n",
    "                        logger.warning(\"‚ö†Ô∏è  Login automation encountered an issue\")\n",
    "                        login_result['error'] = \"Login automation failed\"\n",
    "                        login_result['security_notes'].append(\"Failed login attempt contained within session\")\n",
    "                \n",
    "                else:\n",
    "                    logger.error(\"‚ùå Failed to navigate to login page\")\n",
    "                    login_result['error'] = \"Navigation failed\"\n",
    "                    login_result['security_notes'].append(\"Navigation failure contained within session\")\n",
    "                \n",
    "                # Security cleanup happens automatically when context managers exit\n",
    "                logger.info(\"üßπ AgentCore will automatically clean up browser session and protect data\")\n",
    "                login_result['security_notes'].append(\"Automatic secure cleanup by AgentCore\")\n",
    "                \n",
    "    except ActAgentError as e:\n",
    "        logger.error(f\"‚ùå NovaAct AI processing error: {str(e)}\")\n",
    "        login_result['error'] = f\"NovaAct error: {str(e)}\"\n",
    "        login_result['security_notes'].append(\"Error contained within isolated session\")\n",
    "        \n",
    "    except Exception as e:\n",
    "        logger.error(f\"‚ùå Unexpected error during secure login: {str(e)}\")\n",
    "        login_result['error'] = f\"System error: {str(e)}\"\n",
    "        login_result['security_notes'].append(\"All errors contained within AgentCore session\")\n",
    "    \n",
    "    return login_result\n",
    "\n",
    "# Example usage with a demo login site\n",
    "print(\"üîê Demonstrating secure login automation with NovaAct and AgentCore...\\n\")\n",
    "\n",
    "# Using httpbin.org for a safe demo (it has forms but no actual login)\n",
    "demo_result = secure_login_with_novaact_agentcore(\n",
    "    login_url=\"https://httpbin.org/forms/post\",  # Demo form page\n",
    "    username=\"demo_user\",\n",
    "    password=\"demo_password\"\n",
    ")\n",
    "\n",
    "print(f\"\\nüìä Secure Login Results:\")\n",
    "print(json.dumps(demo_result, indent=2))\n",
    "\n",
    "print(f\"\\nüõ°Ô∏è  Security Features Demonstrated:\")\n",
    "for note in demo_result['security_notes']:\n",
    "    print(f\"   ‚Ä¢ {note}\")\n",
    "\n",
    "if demo_result['navigation_successful']:\n",
    "    print(\"\\n‚úÖ NovaAct successfully processed natural language instructions within AgentCore!\")\n",
    "else:\n",
    "    print(\"\\n‚ö†Ô∏è  Demo completed - check results above for details\")"
   ]
