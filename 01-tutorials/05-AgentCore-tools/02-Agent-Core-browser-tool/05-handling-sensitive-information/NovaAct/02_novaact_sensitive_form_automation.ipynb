    "import hashlib\n",
    "import uuid\n",
    "from datetime import datetime\n",
    "from typing import Any, Dict, List, Optional\n",
    "\n",
    "class SecureLogger:\n",
    "    \"\"\"\n",
    "    Secure logging utility for NovaAct-AgentCore operations with sensitive data.\n",
    "    \n",
    "    This logger ensures that sensitive information is never exposed in logs\n",
    "    while maintaining operational visibility and debugging capabilities.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, session_id: Optional[str] = None):\n",
    "        self.session_id = session_id or str(uuid.uuid4())[:8]\n",
    "        self.operation_count = 0\n",
    "        \n",
    "    def _sanitize_data(self, data: Any) -> str:\n",
    "        \"\"\"\n",
    "        Sanitize sensitive data for safe logging.\n",
    "        \n",
    "        Returns data type and length information instead of actual values.\n",
    "        \"\"\"\n",
    "        if isinstance(data, str):\n",
    "            if len(data) > 50:  # Likely sensitive if very long\n",
    "                return f\"<string:length={len(data)}:hash={hashlib.md5(data.encode()).hexdigest()[:8]}>\"\n",
    "            elif any(keyword in data.lower() for keyword in ['password', 'card', 'ssn', 'credit']):\n",
    "                return f\"<sensitive_string:length={len(data)}>\"\n",
    "            else:\n",
    "                return f\"<string:length={len(data)}>\"\n",
    "        elif isinstance(data, dict):\n",
    "            return f\"<dict:keys={len(data)}:fields={list(data.keys())}>\"\n",
    "        elif isinstance(data, list):\n",
    "            return f\"<list:length={len(data)}>\"\n",
    "        else:\n",
    "            return f\"<{type(data).__name__}>\"\n",
    "    \n",
    "    def log_operation_start(self, operation_type: str, target_url: str, data_types: List[str]):\n",
    "        \"\"\"Log the start of a sensitive data operation.\"\"\"\n",
    "        self.operation_count += 1\n",
    "        logger.info(\n",
    "            f\"üîÑ [{self.session_id}] Operation {self.operation_count} START: {operation_type}\"\n",
    "        )\n",
    "        logger.info(f\"   Target: {target_url}\")\n",
    "        logger.info(f\"   Data Types: {data_types}\")\n",
    "        logger.info(f\"   Timestamp: {datetime.now().isoformat()}\")\n",
    "    \n",
    "    def log_operation_success(self, operation_type: str, result_summary: Dict[str, Any]):\n",
    "        \"\"\"Log successful completion of a sensitive data operation.\"\"\"\n",
    "        sanitized_summary = {k: self._sanitize_data(v) for k, v in result_summary.items()}\n",
    "        logger.info(\n",
    "            f\"‚úÖ [{self.session_id}] Operation {self.operation_count} SUCCESS: {operation_type}\"\n",
    "        )\n",
    "        logger.info(f\"   Result Summary: {sanitized_summary}\")\n",
    "    \n",
    "    def log_operation_failure(self, operation_type: str, error_type: str, error_context: Dict[str, Any]):\n",
    "        \"\"\"Log failure of a sensitive data operation without exposing sensitive information.\"\"\"\n",
    "        sanitized_context = {k: self._sanitize_data(v) for k, v in error_context.items()}\n",
    "        logger.error(\n",
    "            f\"‚ùå [{self.session_id}] Operation {self.operation_count} FAILED: {operation_type}\"\n",
    "        )\n",
    "        logger.error(f\"   Error Type: {error_type}\")\n",
    "        logger.error(f\"   Context: {sanitized_context}\")\n",
    "        logger.info(f\"   üõ°Ô∏è Sensitive data protection maintained during failure\")\n",
    "    \n",
    "    def log_agentcore_session_info(self, session_info: Dict[str, Any]):\n",
    "        \"\"\"Log AgentCore session information for monitoring and debugging.\"\"\"\n",
    "        logger.info(f\"üîí [{self.session_id}] AgentCore Session Info:\")\n",
    "        for key, value in session_info.items():\n",
    "            if 'sensitive' not in key.lower() and 'secret' not in key.lower():\n",
    "                logger.info(f\"   {key}: {value}\")\n",
    "            else:\n",
    "                logger.info(f\"   {key}: <protected>\")\n",
    "\n",
