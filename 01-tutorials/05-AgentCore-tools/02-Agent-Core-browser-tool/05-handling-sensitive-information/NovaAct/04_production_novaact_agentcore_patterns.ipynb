    "       {\n",
    "         \"Effect\": \"Allow\",\n",
    "         \"Action\": [\n",
    "           \"secretsmanager:GetSecretValue\"\n",
    "         ],\n",
    "         \"Resource\": [\n",
    "           \"arn:aws:secretsmanager:*:*:secret:novaact/*\",\n",
    "           \"arn:aws:secretsmanager:*:*:secret:agentcore/*\"\n",
