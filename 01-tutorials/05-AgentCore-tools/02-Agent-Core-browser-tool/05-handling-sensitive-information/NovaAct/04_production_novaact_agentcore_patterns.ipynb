    "## 6. Production Monitoring and Alerting\n",
    "\n",
    "Implement comprehensive monitoring and alerting for NovaAct operations within AgentCore Browser Tool using CloudWatch."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class ProductionMonitoringManager:\n",
    "    \"\"\"\n",
    "    Production monitoring and alerting for NovaAct-AgentCore Browser Tool integration.\n",
    "    \n",
    "    Provides comprehensive monitoring, alerting, and compliance logging\n",
    "    for sensitive data operations in production environments.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, region: str = 'us-east-1'):\n",
    "        self.region = region\n",
    "        self.cloudwatch = boto3.client('cloudwatch', region_name=region)\n",
    "        self.logs_client = boto3.client('logs', region_name=region)\n",
    "        self.sns = boto3.client('sns', region_name=region)\n",
    "        \n",
    "        # Monitoring configuration\n",
    "        self.namespace = 'NovaAct/AgentCore/Production'\n",
    "        self.log_group_name = '/aws/novaact-agentcore/production'\n",
    "        self.alert_topic_arn = None  # Set this to your SNS topic ARN\n",
    "        \n",
    "        # Initialize log group\n",
    "        self._ensure_log_group_exists()\n",
    "    \n",
    "    def _ensure_log_group_exists(self):\n",
    "        \"\"\"\n",
    "        Ensure CloudWatch log group exists for production logging.\n",
    "        \"\"\"\n",
    "        try:\n",
    "            self.logs_client.create_log_group(\n",
    "                logGroupName=self.log_group_name,\n",
    "                tags={\n",
    "                    'Application': 'NovaAct-AgentCore',\n",
    "                    'Environment': 'Production',\n",
    "                    'Purpose': 'SensitiveDataOperations'\n",
    "                }\n",
    "            )\n",
    "            logger.info(f\"Created CloudWatch log group: {self.log_group_name}\")\n",
    "        except self.logs_client.exceptions.ResourceAlreadyExistsException:\n",
    "            logger.debug(f\"Log group already exists: {self.log_group_name}\")\n",
    "        except Exception as e:\n",
    "            logger.warning(f\"Failed to create log group: {str(e)}\")\n",
    "    \n",
    "    def setup_production_alarms(\n",
    "        self,\n",
    "        alert_topic_arn: str,\n",
    "        failure_threshold: int = 5,\n",
    "        latency_threshold: float = 30.0\n",
    "    ) -> List[str]:\n",
    "        \"\"\"\n",
    "        Set up CloudWatch alarms for NovaAct-AgentCore production monitoring.\n",
    "        \n",
    "        Args:\n",
    "            alert_topic_arn: SNS topic ARN for alert notifications\n",
    "            failure_threshold: Number of failures before alerting\n",
    "            latency_threshold: Maximum acceptable task execution time (seconds)\n",
    "            \n",
    "        Returns:\n",
    "            List of created alarm ARNs\n",
    "        \"\"\"\n",
    "        self.alert_topic_arn = alert_topic_arn\n",
    "        created_alarms = []\n",
    "        \n",
    "        # Alarm 1: High failure rate\n",
    "        failure_alarm = self._create_alarm(\n",
    "            alarm_name='NovaAct-AgentCore-HighFailureRate',\n",
    "            alarm_description='High failure rate for NovaAct operations in AgentCore',\n",
    "            metric_name='TaskFailed',\n",
    "            statistic='Sum',\n",
    "            threshold=failure_threshold,\n",
    "            comparison_operator='GreaterThanThreshold',\n",
    "            evaluation_periods=2,\n",
    "            period=300  # 5 minutes\n",
    "        )\n",
    "        if failure_alarm:\n",
    "            created_alarms.append(failure_alarm)\n",
    "        \n",
    "        # Alarm 2: High latency\n",
    "        latency_alarm = self._create_alarm(\n",
    "            alarm_name='NovaAct-AgentCore-HighLatency',\n",
    "            alarm_description='High execution latency for NovaAct tasks in AgentCore',\n",
    "            metric_name='TaskExecutionTime',\n",
    "            statistic='Average',\n",
    "            threshold=latency_threshold,\n",
    "            comparison_operator='GreaterThanThreshold',\n",
    "            evaluation_periods=3,\n",
    "            period=300\n",
    "        )\n",
    "        if latency_alarm:\n",
    "            created_alarms.append(latency_alarm)\n",
    "        \n",
    "        # Alarm 3: Session creation failures\n",
    "        session_alarm = self._create_alarm(\n",
    "            alarm_name='NovaAct-AgentCore-SessionFailures',\n",
    "            alarm_description='AgentCore session creation failures for NovaAct',\n",
    "            metric_name='SessionFailed',\n",
    "            statistic='Sum',\n",
    "            threshold=3,\n",
    "            comparison_operator='GreaterThanThreshold',\n",
    "            evaluation_periods=1,\n",
    "            period=300\n",
    "        )\n",
    "        if session_alarm:\n",
    "            created_alarms.append(session_alarm)\n",
    "        \n",
    "        # Alarm 4: No activity (potential system issue)\n",
    "        activity_alarm = self._create_alarm(\n",
    "            alarm_name='NovaAct-AgentCore-NoActivity',\n",
    "            alarm_description='No NovaAct activity detected in AgentCore sessions',\n",
    "            metric_name='SessionStarted',\n",
    "            statistic='Sum',\n",
    "            threshold=1,\n",
    "            comparison_operator='LessThanThreshold',\n",
    "            evaluation_periods=3,\n",
    "            period=600,  # 10 minutes\n",
    "            treat_missing_data='breaching'\n",
    "        )\n",
    "        if activity_alarm:\n",
    "            created_alarms.append(activity_alarm)\n",
    "        \n",
    "        logger.info(f\"Created {len(created_alarms)} CloudWatch alarms for production monitoring\")\n",
    "        return created_alarms\n",
    "    \n",
    "    def _create_alarm(\n",
    "        self,\n",
    "        alarm_name: str,\n",
    "        alarm_description: str,\n",
    "        metric_name: str,\n",
    "        statistic: str,\n",
    "        threshold: float,\n",
    "        comparison_operator: str,\n",
    "        evaluation_periods: int,\n",
    "        period: int,\n",
    "        treat_missing_data: str = 'notBreaching'\n",
    "    ) -> Optional[str]:\n",
    "        \"\"\"\n",
    "        Create a CloudWatch alarm for NovaAct-AgentCore monitoring.\n",
    "        \n",
    "        Returns:\n",
    "            Alarm ARN if successful, None otherwise\n",
    "        \"\"\"\n",
    "        try:\n",
    "            response = self.cloudwatch.put_metric_alarm(\n",
    "                AlarmName=alarm_name,\n",
    "                AlarmDescription=alarm_description,\n",
    "                ActionsEnabled=True,\n",
    "                AlarmActions=[self.alert_topic_arn] if self.alert_topic_arn else [],\n",
    "                MetricName=metric_name,\n",
    "                Namespace=self.namespace,\n",
    "                Statistic=statistic,\n",
    "                Threshold=threshold,\n",
    "                ComparisonOperator=comparison_operator,\n",
    "                EvaluationPeriods=evaluation_periods,\n",
    "                Period=period,\n",
    "                TreatMissingData=treat_missing_data,\n",
    "                Tags=[\n",
    "                    {\n",
    "                        'Key': 'Application',\n",
    "                        'Value': 'NovaAct-AgentCore'\n",
    "                    },\n",
    "                    {\n",
    "                        'Key': 'Environment',\n",
    "                        'Value': 'Production'\n",
    "                    }\n",
    "                ]\n",
    "            )\n",
    "            \n",
    "            alarm_arn = f\"arn:aws:cloudwatch:{self.region}:{boto3.client('sts').get_caller_identity()['Account']}:alarm:{alarm_name}\"\n",
    "            logger.info(f\"Created alarm: {alarm_name}\")\n",
    "            return alarm_arn\n",
    "            \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Failed to create alarm {alarm_name}: {str(e)}\")\n",
    "            return None\n",
    "    \n",
    "    def log_sensitive_operation(\n",
    "        self,\n",
    "        session_id: str,\n",
    "        operation_type: str,\n",
    "        operation_status: str,\n",
    "        metadata: Dict[str, Any],\n",
    "        compliance_level: str = 'HIGH'\n",
    "    ):\n",
    "        \"\"\"\n",
    "        Log sensitive data operations for compliance and auditing.\n",
    "        \n",
    "        Args:\n",
    "            session_id: AgentCore session identifier\n",
    "            operation_type: Type of operation (login, form_fill, data_extract)\n",
    "            operation_status: SUCCESS, FAILED, or PARTIAL\n",
    "            metadata: Additional operation metadata (sanitized)\n",
    "            compliance_level: Compliance logging level (HIGH, MEDIUM, LOW)\n",
    "        \"\"\"\n",
    "        \n",
    "        # Create compliance log entry (no sensitive data)\n",
    "        log_entry = {\n",
    "            'timestamp': datetime.utcnow().isoformat(),\n",
    "            'session_id': session_id,\n",
    "            'operation_type': operation_type,\n",
    "            'operation_status': operation_status,\n",
    "            'compliance_level': compliance_level,\n",
    "            'agentcore_session_isolated': True,\n",
    "            'novaact_ai_processing': True,\n",
    "            'sensitive_data_protected': True,\n",
    "            'metadata': self._sanitize_metadata(metadata)\n",
    "        }\n",
    "        \n",
    "        try:\n",
    "            # Send to CloudWatch Logs\n",
    "            self.logs_client.put_log_events(\n",
    "                logGroupName=self.log_group_name,\n",
    "                logStreamName=f\"compliance-{datetime.utcnow().strftime('%Y-%m-%d')}\",\n",
    "                logEvents=[\n",
    "                    {\n",
    "                        'timestamp': int(time.time() * 1000),\n",
    "                        'message': json.dumps(log_entry)\n",
    "                    }\n",
    "                ]\n",
    "            )\n",
    "            \n",
    "            logger.info(f\"Compliance log entry created for session {session_id}\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Failed to create compliance log entry: {str(e)}\")\n",
    "    \n",
    "    def _sanitize_metadata(self, metadata: Dict[str, Any]) -> Dict[str, Any]:\n",
    "        \"\"\"\n",
    "        Sanitize metadata to remove sensitive information for compliance logging.\n",
    "        \n",
    "        Args:\n",
    "            metadata: Raw metadata dictionary\n",
    "            \n",
    "        Returns:\n",
    "            Sanitized metadata safe for logging\n",
    "        \"\"\"\n",
    "        sanitized = {}\n",
    "        \n",
    "        # Safe fields that can be logged\n",
    "        safe_fields = {\n",
    "            'execution_time', 'attempts', 'success', 'error_type',\n",
    "            'task_type', 'browser_version', 'region', 'retry_count'\n",
    "        }\n",
    "        \n",
    "        for key, value in metadata.items():\n",
    "            if key in safe_fields:\n",
    "                sanitized[key] = value\n",
    "            elif key.endswith('_count') or key.endswith('_duration'):\n",
    "                sanitized[key] = value\n",
    "            else:\n",
    "                # Replace sensitive data with placeholder\n",
    "                sanitized[f\"{key}_redacted\"] = \"[REDACTED_FOR_COMPLIANCE]\"\n",
    "        \n",
    "        return sanitized\n",
    "    \n",
    "    def create_dashboard(\n",
    "        self,\n",
    "        dashboard_name: str = 'NovaAct-AgentCore-Production'\n",
    "    ) -> str:\n",
    "        \"\"\"\n",
    "        Create a CloudWatch dashboard for NovaAct-AgentCore production monitoring.\n",
    "        \n",
    "        Args:\n",
    "            dashboard_name: Name for the CloudWatch dashboard\n",
    "            \n",
    "        Returns:\n",
    "            Dashboard URL\n",
    "        \"\"\"\n",
    "        \n",
    "        dashboard_body = {\n",
    "            \"widgets\": [\n",
    "                {\n",
    "                    \"type\": \"metric\",\n",
    "                    \"x\": 0,\n",
    "                    \"y\": 0,\n",
    "                    \"width\": 12,\n",
    "                    \"height\": 6,\n",
    "                    \"properties\": {\n",
    "                        \"metrics\": [\n",
    "                            [self.namespace, \"SessionStarted\"],\n",
    "                            [\".\", \"SessionEnded\"],\n",
    "                            [\".\", \"SessionFailed\"]\n",
    "                        ],\n",
    "                        \"period\": 300,\n",
    "                        \"stat\": \"Sum\",\n",
    "                        \"region\": self.region,\n",
    "                        \"title\": \"AgentCore Session Activity\",\n",
    "                        \"view\": \"timeSeries\"\n",
    "                    }\n",
    "                },\n",
    "                {\n",
    "                    \"type\": \"metric\",\n",
    "                    \"x\": 12,\n",
    "                    \"y\": 0,\n",
    "                    \"width\": 12,\n",
    "                    \"height\": 6,\n",
    "                    \"properties\": {\n",
    "                        \"metrics\": [\n",
    "                            [self.namespace, \"TaskSuccess\"],\n",
    "                            [\".\", \"TaskFailed\"]\n",
    "                        ],\n",
    "                        \"period\": 300,\n",
    "                        \"stat\": \"Sum\",\n",
    "                        \"region\": self.region,\n",
    "                        \"title\": \"NovaAct Task Results\",\n",
    "                        \"view\": \"timeSeries\"\n",
    "                    }\n",
    "                },\n",
    "                {\n",
    "                    \"type\": \"metric\",\n",
    "                    \"x\": 0,\n",
    "                    \"y\": 6,\n",
    "                    \"width\": 12,\n",
    "                    \"height\": 6,\n",
    "                    \"properties\": {\n",
    "                        \"metrics\": [\n",
    "                            [self.namespace, \"TaskExecutionTime\"]\n",
    "                        ],\n",
    "                        \"period\": 300,\n",
    "                        \"stat\": \"Average\",\n",
    "                        \"region\": self.region,\n",
    "                        \"title\": \"NovaAct Task Execution Time\",\n",
    "                        \"view\": \"timeSeries\",\n",
    "                        \"yAxis\": {\n",
    "                            \"left\": {\n",
    "                                \"min\": 0\n",
    "                            }\n",
    "                        }\n",
    "                    }\n",
    "                },\n",
    "                {\n",
    "                    \"type\": \"metric\",\n",
    "                    \"x\": 12,\n",
    "                    \"y\": 6,\n",
    "                    \"width\": 12,\n",
    "                    \"height\": 6,\n",
    "                    \"properties\": {\n",
    "                        \"metrics\": [\n",
    "                            [self.namespace, \"SessionDuration\"]\n",
    "                        ],\n",
    "                        \"period\": 300,\n",
    "                        \"stat\": \"Average\",\n",
    "                        \"region\": self.region,\n",
    "                        \"title\": \"AgentCore Session Duration\",\n",
    "                        \"view\": \"timeSeries\"\n",
    "                    }\n",
    "                }\n",
    "            ]\n",
    "        }\n",
    "        \n",
    "        try:\n",
    "            self.cloudwatch.put_dashboard(\n",
    "                DashboardName=dashboard_name,\n",
    "                DashboardBody=json.dumps(dashboard_body)\n",
    "            )\n",
    "            \n",
    "            dashboard_url = f\"https://{self.region}.console.aws.amazon.com/cloudwatch/home?region={self.region}#dashboards:name={dashboard_name}\"\n",
    "            logger.info(f\"Created CloudWatch dashboard: {dashboard_name}\")\n",
    "            return dashboard_url\n",
    "            \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Failed to create dashboard: {str(e)}\")\n",
    "            return \"\"\n",
    "    \n",
    "    def get_production_health_status(self) -> Dict[str, Any]:\n",
    "        \"\"\"\n",
    "        Get current production health status for NovaAct-AgentCore integration.\n",
    "        \n",
    "        Returns:\n",
    "            Dictionary containing health metrics and status\n",
    "        \"\"\"\n",
    "        try:\n",
    "            # Get recent metrics (last 15 minutes)\n",
    "            end_time = datetime.utcnow()\n",
    "            start_time = end_time - timedelta(minutes=15)\n",
    "            \n",
    "            # Query success rate\n",
    "            success_metrics = self.cloudwatch.get_metric_statistics(\n",
    "                Namespace=self.namespace,\n",
    "                MetricName='TaskSuccess',\n",
    "                StartTime=start_time,\n",
    "                EndTime=end_time,\n",
    "                Period=900,  # 15 minutes\n",
    "                Statistics=['Sum']\n",
    "            )\n",
    "            \n",
    "            failure_metrics = self.cloudwatch.get_metric_statistics(\n",
    "                Namespace=self.namespace,\n",
    "                MetricName='TaskFailed',\n",
    "                StartTime=start_time,\n",
    "                EndTime=end_time,\n",
    "                Period=900,\n",
    "                Statistics=['Sum']\n",
    "            )\n",
    "            \n",
    "            # Calculate success rate\n",
    "            total_success = sum(point['Sum'] for point in success_metrics['Datapoints'])\n",
    "            total_failures = sum(point['Sum'] for point in failure_metrics['Datapoints'])\n",
    "            total_operations = total_success + total_failures\n",
    "            \n",
    "            success_rate = (total_success / total_operations * 100) if total_operations > 0 else 0\n",
    "            \n",
    "            # Determine health status\n",
    "            if success_rate >= 95:\n",
    "                health_status = 'HEALTHY'\n",
    "            elif success_rate >= 85:\n",
    "                health_status = 'WARNING'\n",
    "            else:\n",
    "                health_status = 'CRITICAL'\n",
    "            \n",
    "            return {\n",
    "                'health_status': health_status,\n",
    "                'success_rate': round(success_rate, 2),\n",
    "                'total_operations': total_operations,\n",
    "                'successful_operations': total_success,\n",
    "                'failed_operations': total_failures,\n",
    "                'monitoring_period': '15 minutes',\n",
    "                'timestamp': datetime.utcnow().isoformat()\n",
    "            }\n",
    "            \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Failed to get health status: {str(e)}\")\n",
    "            return {\n",
    "                'health_status': 'UNKNOWN',\n",
    "                'error': str(e),\n",
    "                'timestamp': datetime.utcnow().isoformat()\n",
    "            }\n",
    "\n",
