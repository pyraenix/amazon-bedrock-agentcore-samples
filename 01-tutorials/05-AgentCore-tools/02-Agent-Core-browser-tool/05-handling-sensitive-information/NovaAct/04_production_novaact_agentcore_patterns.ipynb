    "## 2. AWS Secrets Manager Integration\n",
    "\n",
    "Implement secure credential management for both NovaAct API keys and AgentCore authentication using AWS Secrets Manager."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class ProductionCredentialManager:\n",
    "    \"\"\"\n",
    "    Production credential management for NovaAct-AgentCore integration.\n",
    "    \n",
    "    Securely manages both NovaAct API keys and AgentCore credentials\n",
    "    using AWS Secrets Manager with proper rotation and caching.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(self, region: str = 'us-east-1'):\n",
    "        self.region = region\n",
    "        self.secrets_client = boto3.client('secretsmanager', region_name=region)\n",
    "        self._credential_cache = {}\n",
    "        self._cache_ttl = 300  # 5 minutes cache TTL\n",
    "        \n",
    "    def get_novaact_credentials(self, secret_name: str = 'novaact/api-key') -> str:\n",
    "        \"\"\"\n",
    "        Retrieve NovaAct API key from AWS Secrets Manager.\n",
    "        \n",
    "        Args:\n",
    "            secret_name: Name of the secret containing NovaAct API key\n",
    "            \n",
    "        Returns:\n",
    "            NovaAct API key string\n",
    "        \"\"\"\n",
    "        return self._get_secret_value(secret_name)\n",
    "    \n",
    "    def get_agentcore_config(self, secret_name: str = 'agentcore/browser-tool-config') -> Dict[str, Any]:\n",
    "        \"\"\"\n",
    "        Retrieve AgentCore Browser Tool configuration from AWS Secrets Manager.\n",
    "        \n",
    "        Args:\n",
    "            secret_name: Name of the secret containing AgentCore config\n",
    "            \n",
    "        Returns:\n",
    "            Dictionary containing AgentCore configuration\n",
    "        \"\"\"\n",
    "        config_json = self._get_secret_value(secret_name)\n",
    "        return json.loads(config_json)\n",
    "    \n",
    "    def _get_secret_value(self, secret_name: str) -> str:\n",
    "        \"\"\"\n",
    "        Internal method to retrieve and cache secret values.\n",
    "        \n",
    "        Implements caching with TTL to reduce Secrets Manager API calls\n",
    "        while maintaining security through regular credential refresh.\n",
    "        \"\"\"\n",
    "        current_time = time.time()\n",
    "        \n",
    "        # Check cache first\n",
    "        if secret_name in self._credential_cache:\n",
    "            cached_value, cache_time = self._credential_cache[secret_name]\n",
    "            if current_time - cache_time < self._cache_ttl:\n",
    "                logger.debug(f\"Using cached credential for {secret_name}\")\n",
    "                return cached_value\n",
    "        \n",
    "        try:\n",
    "            # Retrieve from Secrets Manager\n",
    "            response = self.secrets_client.get_secret_value(SecretId=secret_name)\n",
    "            secret_value = response['SecretString']\n",
    "            \n",
    "            # Cache the value\n",
    "            self._credential_cache[secret_name] = (secret_value, current_time)\n",
    "            \n",
    "            logger.info(f\"Successfully retrieved credential from Secrets Manager: {secret_name}\")\n",
    "            return secret_value\n",
    "            \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Failed to retrieve secret {secret_name}: {str(e)}\")\n",
    "            raise\n",
    "    \n",
    "    def rotate_credentials(self, secret_name: str) -> bool:\n",
    "        \"\"\"\n",
    "        Trigger credential rotation for production security.\n",
    "        \n",
    "        Args:\n",
    "            secret_name: Name of the secret to rotate\n",
    "            \n",
    "        Returns:\n",
    "            True if rotation was successful\n",
    "        \"\"\"\n",
    "        try:\n",
    "            self.secrets_client.rotate_secret(SecretId=secret_name)\n",
    "            # Clear cache to force refresh\n",
    "            if secret_name in self._credential_cache:\n",
    "                del self._credential_cache[secret_name]\n",
    "            \n",
    "            logger.info(f\"Credential rotation initiated for {secret_name}\")\n",
    "            return True\n",
    "            \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Failed to rotate credentials for {secret_name}: {str(e)}\")\n",
    "            return False\n",
    "\n",
    "# Initialize production credential manager\n",
    "credential_manager = ProductionCredentialManager()\n",
    "\n",
    "print(\"✅ Production credential management initialized\")\n",
    "print(\"📋 Credential manager supports:\")\n",
    "print(\"   - Secure NovaAct API key retrieval\")\n",
    "print(\"   - AgentCore configuration management\")\n",
    "print(\"   - Automatic credential caching with TTL\")\n",
    "print(\"   - Production credential rotation\")"
   ]
  },
