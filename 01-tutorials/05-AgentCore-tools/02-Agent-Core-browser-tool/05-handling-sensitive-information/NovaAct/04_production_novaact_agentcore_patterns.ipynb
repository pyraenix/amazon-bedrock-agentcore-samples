    "## 3. Production NovaAct-AgentCore Session Manager\n",
    "\n",
    "Implement a production-ready session manager that handles scaling, error recovery, and resource management."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "class ProductionNovaActAgentCoreManager:\n",
    "    \"\"\"\n",
    "    Production-ready manager for NovaAct-AgentCore Browser Tool integration.\n",
    "    \n",
    "    Handles scaling, monitoring, error recovery, and resource management\n",
    "    for enterprise-grade browser automation workflows.\n",
    "    \"\"\"\n",
    "    \n",
    "    def __init__(\n",
    "        self,\n",
    "        credential_manager: ProductionCredentialManager,\n",
    "        region: str = 'us-east-1',\n",
    "        max_concurrent_sessions: int = 10,\n",
    "        session_timeout: int = 300,\n",
    "        retry_attempts: int = 3\n",
    "    ):\n",
    "        self.credential_manager = credential_manager\n",
    "        self.region = region\n",
    "        self.max_concurrent_sessions = max_concurrent_sessions\n",
    "        self.session_timeout = session_timeout\n",
    "        self.retry_attempts = retry_attempts\n",
    "        \n",
    "        # Production monitoring\n",
    "        self.cloudwatch = boto3.client('cloudwatch', region_name=region)\n",
    "        self.active_sessions = {}\n",
    "        self.session_metrics = {\n",
    "            'total_sessions': 0,\n",
    "            'successful_operations': 0,\n",
    "            'failed_operations': 0,\n",
    "            'average_session_duration': 0\n",
    "        }\n",
    "        \n",
    "    @contextmanager\n",
    "    def production_session(\n",
    "        self,\n",
    "        session_id: Optional[str] = None,\n",
    "        enable_monitoring: bool = True\n",
    "    ):\n",
    "        \"\"\"\n",
    "        Create a production NovaAct-AgentCore session with full monitoring.\n",
    "        \n",
    "        Args:\n",
    "            session_id: Optional session identifier for tracking\n",
    "            enable_monitoring: Whether to enable CloudWatch monitoring\n",
    "            \n",
    "        Yields:\n",
    "            Tuple of (agentcore_client, nova_act_client)\n",
    "        \"\"\"\n",
    "        if session_id is None:\n",
    "            session_id = f\"session_{int(time.time())}_{os.getpid()}\"\n",
    "        \n",
    "        session_start_time = time.time()\n",
    "        \n",
    "        try:\n",
    "            # Get production credentials\n",
    "            novaact_api_key = self.credential_manager.get_novaact_credentials()\n",
    "            agentcore_config = self.credential_manager.get_agentcore_config()\n",
    "            \n",
    "            logger.info(f\"Starting production session {session_id}\")\n",
    "            \n",
    "            # Create AgentCore Browser Tool session with production config\n",
    "            with browser_session(\n",
    "                region=self.region,\n",
    "                **agentcore_config\n",
    "            ) as agentcore_client:\n",
    "                \n",
    "                # Get secure CDP connection\n",
    "                ws_url, headers = agentcore_client.generate_ws_headers()\n",
    "                \n",
    "                # Connect NovaAct to AgentCore's managed browser\n",
    "                with NovaAct(\n",
    "                    cdp_endpoint_url=ws_url,\n",
    "                    cdp_headers=headers,\n",
    "                    nova_act_api_key=novaact_api_key,\n",
    "                    timeout=self.session_timeout\n",
    "                ) as nova_act:\n",
    "                    \n",
    "                    # Register active session\n",
    "                    self.active_sessions[session_id] = {\n",
    "                        'start_time': session_start_time,\n",
    "                        'agentcore_client': agentcore_client,\n",
    "                        'nova_act': nova_act\n",
    "                    }\n",
    "                    \n",
    "                    # Send session start metric\n",
    "                    if enable_monitoring:\n",
    "                        self._send_metric('SessionStarted', 1, session_id)\n",
    "                    \n",
    "                    logger.info(f\"Production session {session_id} ready - NovaAct connected to AgentCore\")\n",
    "                    \n",
    "                    yield agentcore_client, nova_act\n",
    "                    \n",
    "        except Exception as e:\n",
    "            logger.error(f\"Production session {session_id} failed: {str(e)}\")\n",
    "            if enable_monitoring:\n",
    "                self._send_metric('SessionFailed', 1, session_id)\n",
    "            raise\n",
    "            \n",
    "        finally:\n",
    "            # Calculate session duration\n",
    "            session_duration = time.time() - session_start_time\n",
    "            \n",
    "            # Update metrics\n",
    "            self.session_metrics['total_sessions'] += 1\n",
    "            self._update_average_duration(session_duration)\n",
    "            \n",
    "            # Clean up session tracking\n",
    "            if session_id in self.active_sessions:\n",
    "                del self.active_sessions[session_id]\n",
    "            \n",
    "            # Send session end metric\n",
    "            if enable_monitoring:\n",
    "                self._send_metric('SessionDuration', session_duration, session_id)\n",
    "                self._send_metric('SessionEnded', 1, session_id)\n",
    "            \n",
    "            logger.info(f\"Production session {session_id} completed in {session_duration:.2f}s\")\n",
    "    \n",
    "    def execute_with_retry(\n",
    "        self,\n",
    "        nova_act_client,\n",
    "        task_description: str,\n",
    "        session_id: str,\n",
    "        max_retries: Optional[int] = None\n",
    "    ) -> Dict[str, Any]:\n",
    "        \"\"\"\n",
    "        Execute NovaAct task with automatic retry logic for production reliability.\n",
    "        \n",
    "        Args:\n",
    "            nova_act_client: NovaAct client instance\n",
    "            task_description: Natural language task description\n",
    "            session_id: Session identifier for tracking\n",
    "            max_retries: Maximum retry attempts (defaults to instance setting)\n",
    "            \n",
    "        Returns:\n",
    "            Dictionary containing execution results and metadata\n",
    "        \"\"\"\n",
    "        if max_retries is None:\n",
    "            max_retries = self.retry_attempts\n",
    "        \n",
    "        last_error = None\n",
    "        \n",
    "        for attempt in range(max_retries + 1):\n",
    "            try:\n",
    "                logger.info(f\"Executing NovaAct task (attempt {attempt + 1}/{max_retries + 1}): {task_description[:100]}...\")\n",
    "                \n",
    "                # Execute NovaAct task within AgentCore's secure environment\n",
    "                start_time = time.time()\n",
    "                result = nova_act_client.act(task_description)\n",
    "                execution_time = time.time() - start_time\n",
    "                \n",
    "                if result.success:\n",
    "                    # Success metrics\n",
    "                    self.session_metrics['successful_operations'] += 1\n",
    "                    self._send_metric('TaskSuccess', 1, session_id)\n",
    "                    self._send_metric('TaskExecutionTime', execution_time, session_id)\n",
    "                    \n",
    "                    logger.info(f\"NovaAct task completed successfully in {execution_time:.2f}s\")\n",
    "                    \n",
    "                    return {\n",
    "                        'success': True,\n",
    "                        'result': result,\n",
    "                        'execution_time': execution_time,\n",
    "                        'attempts': attempt + 1,\n",
    "                        'session_id': session_id\n",
    "                    }\n",
    "                else:\n",
    "                    # Task failed but NovaAct responded\n",
    "                    logger.warning(f\"NovaAct task failed: {result.error if hasattr(result, 'error') else 'Unknown error'}\")\n",
    "                    last_error = f\"Task failed: {result.error if hasattr(result, 'error') else 'Unknown error'}\"\n",
    "                    \n",
    "            except ActAgentError as e:\n",
    "                logger.warning(f\"NovaAct error on attempt {attempt + 1}: {str(e)}\")\n",
    "                last_error = str(e)\n",
    "                \n",
    "            except Exception as e:\n",
    "                logger.error(f\"Unexpected error on attempt {attempt + 1}: {str(e)}\")\n",
    "                last_error = str(e)\n",
    "            \n",
    "            # Wait before retry (exponential backoff)\n",
    "            if attempt < max_retries:\n",
    "                wait_time = 2 ** attempt\n",
    "                logger.info(f\"Retrying in {wait_time} seconds...\")\n",
    "                time.sleep(wait_time)\n",
    "        \n",
    "        # All attempts failed\n",
    "        self.session_metrics['failed_operations'] += 1\n",
    "        self._send_metric('TaskFailed', 1, session_id)\n",
    "        \n",
    "        logger.error(f\"NovaAct task failed after {max_retries + 1} attempts: {last_error}\")\n",
    "        \n",
    "        return {\n",
    "            'success': False,\n",
    "            'error': last_error,\n",
    "            'attempts': max_retries + 1,\n",
    "            'session_id': session_id\n",
    "        }\n",
    "    \n",
    "    def _send_metric(self, metric_name: str, value: float, session_id: str):\n",
    "        \"\"\"\n",
    "        Send custom metrics to CloudWatch for monitoring.\n",
    "        \n",
    "        Args:\n",
    "            metric_name: Name of the metric\n",
    "            value: Metric value\n",
    "            session_id: Session identifier for dimensions\n",
    "        \"\"\"\n",
    "        try:\n",
    "            self.cloudwatch.put_metric_data(\n",
    "                Namespace='NovaAct/AgentCore/Production',\n",
    "                MetricData=[\n",
    "                    {\n",
    "                        'MetricName': metric_name,\n",
    "                        'Value': value,\n",
    "                        'Unit': 'Count' if metric_name.endswith(('Started', 'Ended', 'Success', 'Failed')) else 'Seconds',\n",
    "                        'Dimensions': [\n",
    "                            {\n",
    "                                'Name': 'SessionId',\n",
    "                                'Value': session_id\n",
    "                            },\n",
    "                            {\n",
    "                                'Name': 'Region',\n",
    "                                'Value': self.region\n",
    "                            }\n",
    "                        ]\n",
    "                    }\n",
    "                ]\n",
    "            )\n",
    "        except Exception as e:\n",
    "            logger.warning(f\"Failed to send metric {metric_name}: {str(e)}\")\n",
    "    \n",
    "    def _update_average_duration(self, duration: float):\n",
    "        \"\"\"\n",
    "        Update running average of session durations.\n",
    "        \n",
    "        Args:\n",
    "            duration: Session duration in seconds\n",
    "        \"\"\"\n",
    "        total_sessions = self.session_metrics['total_sessions']\n",
    "        current_avg = self.session_metrics['average_session_duration']\n",
    "        \n",
    "        # Calculate new running average\n",
    "        new_avg = ((current_avg * (total_sessions - 1)) + duration) / total_sessions\n",
    "        self.session_metrics['average_session_duration'] = new_avg\n",
    "    \n",
    "    def get_production_metrics(self) -> Dict[str, Any]:\n",
    "        \"\"\"\n",
    "        Get current production metrics and session status.\n",
    "        \n",
    "        Returns:\n",
    "            Dictionary containing production metrics\n",
    "        \"\"\"\n",
    "        return {\n",
    "            'active_sessions': len(self.active_sessions),\n",
    "            'max_concurrent_sessions': self.max_concurrent_sessions,\n",
    "            'session_metrics': self.session_metrics.copy(),\n",
    "            'region': self.region,\n",
    "            'session_timeout': self.session_timeout\n",
    "        }\n",
    "\n",
    "# Initialize production manager\n",
    "production_manager = ProductionNovaActAgentCoreManager(\n",
    "    credential_manager=credential_manager,\n",
    "    max_concurrent_sessions=10,\n",
    "    session_timeout=300,\n",
    "    retry_attempts=3\n",
    ")\n",
    "\n",
    "print(\"✅ Production NovaAct-AgentCore manager initialized\")\n",
    "print(\"📊 Production features enabled:\")\n",
    "print(\"   - Automatic credential management\")\n",
    "print(\"   - Session scaling and resource management\")\n",
    "print(\"   - CloudWatch monitoring and metrics\")\n",
    "print(\"   - Automatic retry with exponential backoff\")\n",
    "print(\"   - Production error handling and logging\")"
   ]
  },
