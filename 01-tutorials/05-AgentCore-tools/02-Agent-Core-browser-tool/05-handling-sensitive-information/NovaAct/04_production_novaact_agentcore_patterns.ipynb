    "import os\n",
    "import json\n",
    "import logging\n",
    "import boto3\n",
    "from datetime import datetime, timedelta\n",
    "from typing import Dict, List, Optional, Any\n",
    "from concurrent.futures import ThreadPoolExecutor, as_completed\n",
    "import time\n",
    "from contextlib import contextmanager\n",
    "\n",
    "# Production imports\n",
    "from bedrock_agentcore.tools.browser_client import browser_session\n",
    "from nova_act import NovaAct, ActAgentError\n",
    "\n",
    "# Configure production logging\n",
    "logging.basicConfig(\n",
    "    level=logging.INFO,\n",
    "    format='%(asctime)s - %(name)s - %(levelname)s - %(message)s',\n",
    "    handlers=[\n",
    "        logging.StreamHandler(),\n",
    "        logging.FileHandler('novaact_agentcore_production.log')\n",
    "    ]\n",
    ")\n",
    "logger = logging.getLogger('NovaActAgentCoreProduction')\n",
    "\n",
    "# Verify environment variables for development/testing\n",
    "required_env_vars = ['NOVA_ACT_API_KEY', 'AWS_REGION']\n",
    "for var in required_env_vars:\n",
    "    if not os.environ.get(var):\n",
    "        print(f\"⚠️  Warning: {var} not found in environment variables\")\n",
    "        print(f\"   For production, this should be managed via AWS Secrets Manager\")\n",
    "    else:\n",
    "        print(f\"✅ {var} found in environment\")\n",
    "\n",
    "print(\"✅ Production environment setup complete\")"
   ]
